<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <variable
            name="info"
            type="io.thoughtbox.hamdan.model.invoice.ConfirmPaymentResponseData" />
    </data>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/ic_bg"
            android:gravity="center">



            <ImageButton
                android:id="@+id/share"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="24dp"
                android:background="@android:color/transparent"
                android:src="@drawable/share_36" />

            <ImageButton
                android:id="@+id/print"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="30dp"
                android:layout_toStartOf="@+id/share"
                android:background="@android:color/transparent"
                android:src="@drawable/ic_print_black_36dp" />

            <ImageButton
                android:id="@+id/closebtn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginStart="24dp"
                android:layout_marginTop="16dp"
                android:background="@android:color/transparent"
                android:src="@drawable/ic_close_36" />

            <androidx.cardview.widget.CardView
                android:id="@+id/view"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/closebtn"
                android:layout_alignParentBottom="true"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="20dp"
                app:cardBackgroundColor="@android:color/transparent"
                app:cardCornerRadius="15dp">

                <ScrollView
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:fillViewport="true">

                    <androidx.cardview.widget.CardView
                        android:id="@+id/card"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        app:cardBackgroundColor="@color/white"
                        app:cardCornerRadius="10dp"
                        app:cardElevation="0dp">


                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical"
                            android:weightSum="1">

                            <LinearLayout
                                android:id="@+id/title_icon2"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_weight="0.3"
                                android:gravity="center"
                                android:orientation="vertical">

                                <TextView
                                    android:id="@+id/total"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginBottom="10dp"
                                    android:gravity="center"
                                    android:text="@{info.lctotal.concat(` `).concat(info.origincurrencycode)}"
                                    android:textColor="@color/colorPrimaryDark"
                                    android:textSize="25sp" />

                                <TextView
                                    android:id="@+id/msg2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"

                                    android:gravity="center"
                                    android:text="@{info.originalstatusmessage}"
                                    android:textColor="#80000000"
                                    android:textSize="14sp"

                                    />

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginEnd="10dp"
                                    android:layout_marginBottom="10dp"

                                    android:gravity="center"
                                    android:orientation="horizontal"
                                    android:weightSum="2">

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"

                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <ImageView
                                            android:id="@+id/barcode"
                                            android:layout_width="wrap_content"
                                            android:layout_height="25dp"
                                            android:layout_gravity="center" />

                                        <TextView
                                            android:id="@+id/refno"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_gravity="center"
                                            android:gravity="start"
                                            android:padding="5dp"
                                            android:paddingStart="20dp"
                                            android:paddingEnd="20dp"
                                            android:text="@{`Ref Num: ` +info.refno}"

                                            android:textSize="12sp" />
                                    </LinearLayout>

                                    <ImageView
                                        android:id="@+id/logo2"
                                        android:layout_width="0dp"
                                        android:layout_height="50dp"
                                        android:layout_gravity="start"
                                        android:layout_weight="1"
                                        android:paddingStart="10dp"
                                        android:paddingEnd="10dp"
                                        android:src="@drawable/ic_report_logo" />
                                </LinearLayout>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_marginTop="10dp"
                                android:layout_weight="0.7"
                                android:gravity="top"
                                android:orientation="vertical">


                                <TextView
                                    android:id="@+id/pin"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="20dp"
                                    android:layout_marginTop="10dp"
                                    android:layout_marginBottom="10dp"
                                    android:gravity="start"
                                    android:text="@{`PIN: `+ info.transactionpin}"
                                    android:textColor="@color/textColor"
                                    android:textSize="16sp" />


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="2">

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/type_lbl"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"
                                            android:gravity="start"
                                            android:text="Transfer Type"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/type"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="10dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.txntype}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />
                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/account_number_lbl"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="Account Number"
                                            android:textSize="12sp" />


                                        <TextView
                                            android:id="@+id/account_number"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="10dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.benaccountno}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />
                                    </LinearLayout>
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="2">

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical"
                                        android:paddingStart="20dp">

                                        <TextView
                                            android:id="@+id/datetime"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginBottom="5dp"


                                            android:gravity="start"
                                            android:text="Date"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/date"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginBottom="10dp"

                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.trxdate}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />
                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView

                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="Time"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/time"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginEnd="10dp"
                                            android:layout_marginBottom="10dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.trxtime}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />
                                    </LinearLayout>
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="2">

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/name"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="TO"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/to_name"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="10dp"

                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.benname}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />
                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/purpose_lbl"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="For"
                                            android:textSize="12sp" />


                                        <TextView
                                            android:id="@+id/purpose"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginEnd="10dp"
                                            android:layout_marginBottom="10dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.purpose}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />
                                    </LinearLayout>
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:gravity="start"
                                    android:orientation="vertical">


                                    <TextView
                                        android:id="@+id/from_lbl"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="20dp"
                                        android:layout_marginBottom="5dp"

                                        android:gravity="start"
                                        android:text="From"
                                        android:textSize="12sp" />

                                    <TextView
                                        android:id="@+id/from_name"
                                        android:layout_width="wrap_content"
                                        android:layout_height="wrap_content"
                                        android:layout_marginStart="20dp"
                                        android:layout_marginBottom="10dp"

                                        android:fontFamily="@font/roboto_bold"
                                        android:gravity="start"
                                        android:textColor="#000000"
                                        android:textSize="12sp" />
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="2">

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/bank_lbl"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="Bank"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/bank"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="10dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.benagent}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />
                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView
                                            android:id="@+id/branch_lbl"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="Branch"
                                            android:textSize="12sp" />


                                        <TextView
                                            android:id="@+id/branch"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginEnd="10dp"
                                            android:layout_marginBottom="10dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.benbranch}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />
                                    </LinearLayout>
                                </LinearLayout>


                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="2">

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView

                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="Tax Payable Amount"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/charge"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="10dp"

                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.charges +` `+ info.origincurrencycode}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />

                                    </LinearLayout>


                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView

                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="VAT"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/amount"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="10dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.vatamount +` `+ info.origincurrencycode}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />

                                    </LinearLayout>


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="2">

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView

                                            android:layout_width="match_parent"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"
                                            android:gravity="start"
                                            android:text="Rebate Amount"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/rebate"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="10dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.rebate+` `+ info.origincurrencycode}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView

                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="Rate"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/rate"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="10dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"

                                            android:text="@{info.rate}"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />

                                    </LinearLayout>
                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="2">

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView

                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="VATIN Number"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/vatTin"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="10dp"

                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="OM1100027028"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:orientation="vertical">

                                        <TextView

                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="TAX CARD Number"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/taxNum"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="10dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="8107014"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />

                                    </LinearLayout>


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:orientation="horizontal"
                                    android:weightSum="2">

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:layout_marginBottom="10dp"
                                        android:orientation="vertical">

                                        <TextView

                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="Amount"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:gravity="start"
                                            android:text="@{info.fcamount+ ` `+ info.bencurrencycode }"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="start"
                                        android:layout_marginBottom="10dp"
                                        android:orientation="vertical">

                                        <TextView

                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"

                                            android:gravity="start"
                                            android:text="Status"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:id="@+id/status"
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginEnd="10dp"

                                            android:fontFamily="@font/roboto_bold"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />

                                    </LinearLayout>


                                </LinearLayout>

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginEnd="10dp"
                                    android:layout_marginBottom="10dp"
                                    android:gravity="center"
                                    android:orientation="horizontal"
                                   >

                                    <LinearLayout
                                        android:layout_width="0dp"
                                        android:layout_height="wrap_content"
                                        android:layout_weight="1"
                                        android:gravity="center|top"
                                        android:orientation="vertical">

                                        <TextView

                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginBottom="5dp"
                                            android:gravity="start"
                                            android:textSize="12sp" />

                                        <TextView
                                            android:layout_width="wrap_content"
                                            android:layout_height="wrap_content"
                                            android:layout_marginStart="20dp"
                                            android:layout_marginEnd="10dp"
                                            android:layout_marginBottom="10dp"
                                            android:fontFamily="@font/roboto_bold"
                                            android:textColor="#000000"
                                            android:textSize="12sp" />
                                    </LinearLayout>


                                </LinearLayout>


                            </LinearLayout>

                        </LinearLayout>


                    </androidx.cardview.widget.CardView>
                </ScrollView>


            </androidx.cardview.widget.CardView>


        </RelativeLayout>
</layout>




